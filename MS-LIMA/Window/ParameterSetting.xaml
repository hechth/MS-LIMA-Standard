<Window x:Class="Metabolomics.MsLima.ParameterSettingWindow"
        Name="ParamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Metabolomics.MsLima"
        mc:Ignorable="d"
        Title="Parameter setting" Height="450" Width="800">
    <Grid>
        <Label Content="In preparation"/>
        <Button Name="ButtonCancel" Content="Cancel" Command="{Binding CancelCommand}" CommandParameter="{Binding ElementName=ParamWindow}" Width="50" Height="25" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,5,5"/>
    </Grid>
</Window>
